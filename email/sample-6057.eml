    <style>
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;
            margin: 0;
            padding: 0;
            background-color: #4D148C; /* Precise FedEx Purple */
            color: #333;
            line-height: 1.6;
        }
        .container {
            padding-top: 10px;
            max-width: 600px;
            margin: 0 auto;
            background-color: white;
            box-shadow: 0 4px 6px rgba(0,0,0,0.1);
        }
        .header {
            background-color: #4D148C;
            color: white;
            text-align: center;
            padding: 15px;
        }
        .logo {
            max-width: 150px;
            margin: 10px auto;
        }
        .package-icon {
            display: block;
            width: 100px;
            height: 100px;
            margin: 20px auto;
            background-color: #4D148C;
            border-radius: 50%;
            position: relative;
        }
        .package-icon::before {
            content: '!';
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: white;
            font-size: 60px;
            font-weight: bold;
        }
        .content {
            padding: 25px;
        }
        .delivery-status {
            text-align: center;
            margin-bottom: 20px;
        }
        .delivery-status h1 {
            color: #4D148C;
            font-size: 24px;
            margin-bottom: 10px;
        }
        .package-details {
            background-color: #f4f4f4;
            padding: 15px;
            border-radius: 8px;
            margin-bottom: 20px;
        }
        .status-update {
            margin-bottom: 15px;
        }
        .status-indicator {
            display: inline-block;
            width: 10px;
            height: 10px;
            border-radius: 50%;
            margin-right: 10px;
            vertical-align: middle;
        }
        .status-green {
            background-color: #2ecc71;
        }
        .status-red {
            background-color: #e74c3c;
        }
        .cta-button {
            display: block;
            width: 250px;
            margin: 20px auto;
            padding: 15px;
            background-color: #FF6600; /* FedEx Orange */
            color: white;
            text-align: center;
            text-decoration: none;
            border-radius: 8px;
            font-weight: bold;
